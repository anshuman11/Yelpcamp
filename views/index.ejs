<h2>WELCOME TO THE INDEX!</h2>
<% blogs.forEach(function(blog){ %>
    <div>
        <h2>
            <%= blog.title %>
        </h2>
        <img src="<%= blog.image %>">
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
        <a href="/blogs/<%= blog._id %>">Read More...</a>
        
    </div><br><br>
    <form action="/blogs/new" method="GET">
        <button>Add_More..</button>
    
<% }) %>