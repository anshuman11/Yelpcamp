<h2>SHOW PAGE!</h2>
<br>
<div>
        <h2>
            <%= blog.title %>
        </h2>
        <img src="<%= blog.image %>">
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
        <form action="/blogs/<%= blog._id %>?_method=DELETE" method="POST">
           <button>Delete</button> 
        </form>
        <form action="/blogs/<%= blog._id %>/edit" method="GET">
            <button>Edit</button>
            </form>
</div>